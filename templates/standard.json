{
  "shell.type" : "bash",
  "define.variables" : {
    "var1": "Hello",
    "var2": "There",
    "var3": "Bye",
    "var4": "There",
    "i": 0,
    "option": "$1"
  },
  "functions": [
  {
    "name":"help_menu",
    "statements": [
      {
        "conditions": [
          {
            "type" : "if",
            "goal" : "1 -gt 1",
            "run" : {
              "printf":"$var1, $var2\\n"
            }
          }
        ]
      },
      {
        "conditions": [
          {
            "type" : "if",
            "goal" : "1 == 1",
            "run" : {
              "printf":"$var3, $var4\\n"
            }
          }
        ]
      },{
        "type" : "command_call",
        "exit": "0"
      }
    ]
  }
  ],
  "conditions" : [
    {
      "type" : "if",
      "goal" : "1 == 1",
      "run" : {
        "printf":"$var1, $var2\\n"
      }
    },
    {
      "type" : "case",
      "goal" : "option",
      "switch" : [
        {
          "-h|-help|--help": "help_menu"
        }
      ]
    },
    {
      "type" : "while",
      "goal" : "$i -lt 10",
      "run" : [
        {
          "type" : "parameter",
          "printf": "loop...\\n"
        },{
          "type" : "declare",
          "i": "$((i+1))"
        }
      ]
    },
    {
      "type" : "for",
      "goal" : "i=0; $i < 10; i++",
      "run" : [
        {
          "type" : "declare",
          "j": "$((j+1))"
        },{
          "type" : "parameter",
          "printf": "loop $j\\n"
        }
      ]
    }
  ]
}