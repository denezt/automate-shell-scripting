{
    "purpose": "Random Math Applications",
    "shell.type": "bash",
    "define.variables": {
        "var1": "1",
        "var2": "2",
        "var3": "3",
        "var4": "4"
    },
    "conditions": [
        {
            "type": "if",
            "goal": "1 -eq 1",
            "run": [
                {
                    "type": "parameter",
                    "printf": "$var1, $var2\\n"
                },
                {
                    "type": "parameter",
                    "printf": "$(($var1 + $var2))\\n"
                },
                {
                    "type": "command_call",
                    "sleep": [
                        "0.",
                        "1"
                    ]
                },
                {
                    "type": "command_call",
                    "sleep": [
                        "0.",
                        "5"
                    ]
                }
            ]
        }
    ]
}